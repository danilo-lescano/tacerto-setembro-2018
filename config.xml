<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sesigames.jogos.testetacerto" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TaCerto</name>
    <description>
        O jogo TaCerto feito para o SESI MS.
    </description>
    <author email="nq3i4fsx@hotmail.com" href="http://cordova.io">
        SESI GAMES
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
    <icon density="mdpi" height="57" platform="android" src="www/resources/media/image/LOGO-ABERTURA.png" width="57" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
</widget>
